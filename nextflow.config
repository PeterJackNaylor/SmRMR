profiles {
    dclasso {
        executor {
            queueSize = 500
            submitRateLimit = '10 sec'
        }
        env {
            PYTHONPATH = "$PWD:$baseDir/templates:\$PYTHONPATH"
        }
        process.conda = "$PWD/env/"
        process.clusterOptions = { task.attempt <= 1 ? '-jc pcc-normal -v PATH=/usr/bin:/home/pnaylor/miniconda3/bin:$PATH,PYTHONPATH=/data/ghighdim/pnaylor/project/dclasso/:/data/ghighdim/pnaylor/project/dclasso/src/templates:$PYTHONPATH' : '-jc pcc-large -v PATH=/usr/bin:/home/pnaylor/miniconda3/bin:$PATH,PYTHONPATH=/data/ghighdim/pnaylor/project/dclasso/:/data/ghighdim/pnaylor/project/dclasso/src/templates:$PYTHONPATH' }
        errorStrategy = 'retry'
        maxRetries = 2
    }
}
