profiles {
    dclasso {
        process.executor = 'sge'
        process.clusterOptions = '-v PATH=/usr/bin:/home/pnaylor/miniconda3/bin:$PATH,PYTHONPATH=/data/ghighdim/pnaylor/project/dclasso/:/data/ghighdim/pnaylor/project/dclasso/src/templates:$PYTHONPATH'
        executor {
            queueSize = 500
            submitRateLimit = '10 sec'
        }
        env {
            PYTHONPATH = "$PWD:$baseDir/templates:\$PYTHONPATH"
        }
        process.conda = "$PWD/env/"
    }
}
